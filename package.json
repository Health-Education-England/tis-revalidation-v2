{
  "name": "revalidation",
  "version": "1.1.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:misc && ng serve",
    "start:mocky": "npm run start -- --configuration=mocky",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build",
    "build:misc": "npm run build:browsers",
    "build:browsers": "node supported.browsers.node.ts",
    "test": "ng test --code-coverage=true",
    "test:ci": "npm run test -- --code-coverage=true --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "cypress": "cypress run --browser chrome",
    "cypress:local": "cypress run --browser chrome --config baseUrl=http://localhost:4200",
    "cypress:alpha": "cypress run --browser chrome --config baseUrl=https://alpha-revalidation.tis.nhs.uk",
    "cypress:open": "cypress open --config baseUrl=http://localhost:4200",
    "tslint-check": "tslint-config-prettier-check ./tslint.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.14",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.14",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@angular/service-worker": "^16.2.12",
    "@ngxs/logger-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "@sentry/browser": "^7.100.1",
    "angular-google-tag-manager": "^1.8.0",
    "angulartics2": "^12.2.0",
    "aws-amplify": "^5.3.15",
    "browserslist": "^4.22.3",
    "file-saver": "^2.0.5",
    "karma-coverage": "^2.2.1",
    "nhsuk-frontend": "^7.1.0",
    "rxjs": "^7.4.0",
    "tslib": "^2.4.0",
    "zone.js": "^0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.12",
    "@angular-devkit/core": "^16.2.12",
    "@angular-eslint/builder": "16.1.0",
    "@angular-eslint/eslint-plugin": "16.1.0",
    "@angular-eslint/eslint-plugin-template": "16.1.0",
    "@angular-eslint/schematics": "16.1.0",
    "@angular-eslint/template-parser": "16.1.0",
    "@angular/cli": "^16.2.12",
    "@angular/compiler-cli": "^16.2.12",
    "@angular/language-service": "^17.2.1",
    "@ngxs/devtools-plugin": "^3.8.2",
    "@react-native-community/netinfo": "^9.5.0",
    "@types/jasmine": "^4.6.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/node": "^18.19.15",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.4.1",
    "browserslist-useragent-regexp": "^3.0.2",
    "buffer": "^6.0.3",
    "codelyzer": "^6.0.0",
    "cypress": "^12.17.4",
    "eslint": "^8.17.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.1",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^16.1.0",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "rollup": "^3.26.3",
    "stylelint": "^15.10.3",
    "stylelint-prettier": "^4.0.0",
    "ts-node": "~10.9.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.6"
  }
}
