{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "revalidation": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [
              "@aws-amplify/auth/lib/types",
              "@aws-amplify/core/lib-esm",
              "@aws-crypto/sha256-browser",
              "@aws-crypto/sha256-js",
              "@aws-sdk/config-resolver",
              "@aws-sdk/credential-provider-cognito-identity",
              "@aws-sdk/eventstream-marshaller",
              "@aws-sdk/eventstream-serde-browser",
              "@aws-sdk/eventstream-serde-config-resolver",
              "@aws-sdk/fetch-http-handler",
              "@aws-sdk/hash-blob-browser",
              "@aws-sdk/md5-js",
              "@aws-sdk/middleware-apply-body-checksum",
              "@aws-sdk/middleware-bucket-endpoint",
              "@aws-sdk/middleware-content-length",
              "@aws-sdk/middleware-expect-continue",
              "@aws-sdk/middleware-host-header",
              "@aws-sdk/middleware-location-constraint",
              "@aws-sdk/middleware-retry",
              "@aws-sdk/middleware-sdk-s3",
              "@aws-sdk/middleware-serde",
              "@aws-sdk/middleware-signing",
              "@aws-sdk/middleware-ssec",
              "@aws-sdk/middleware-user-agent",
              "@aws-sdk/property-provider",
              "@aws-sdk/protocol-http",
              "@aws-sdk/querystring-builder",
              "@aws-sdk/querystring-parser",
              "@aws-sdk/s3-request-presigner",
              "@aws-sdk/smithy-client",
              "@aws-sdk/url-parser-browser",
              "@aws-sdk/url-parser-node",
              "@aws-sdk/util-buffer-from",
              "@aws-sdk/util-create-request",
              "@aws-sdk/util-format-url",
              "@aws-sdk/util-uri-escape",
              "@aws-sdk/util-utf8-browser",
              "@aws-sdk/util-utf8-node",
              "@aws-sdk/xml-builder",
              "aws-sdk",
              "axios",
              "buffer",
              "crypto-js/enc-base64",
              "crypto-js/hmac-sha256",
              "crypto-js/lib-typedarrays",
              "crypto-js/sha256",
              "fast-xml-parser",
              "file-saver",
              "js-cookie",
              "lodash",
              "ulid",
              "uuid",
              "zen-observable",
              "zen-push",
              "zone.js/dist/zone-error"
            ],
            "outputPath": "dist/revalidation",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/nhsuk-frontend/packages/assets/",
                "output": "assets/"
              },
              "src/manifest.webmanifest",
              "src/robots.txt"
            ],
            "styles": ["src/styles.scss"],
            "scripts": [
              {
                "input": "supported.browsers.js",
                "inject": false,
                "bundleName": "supported.browsers"
              }
            ],
            "budgets": [
              {
                "type": "initial",
                "maximumWarning": "30mb",
                "maximumError": "60mb"
              },
              {
                "type": "anyComponentStyle",
                "maximumWarning": "30kb",
                "maximumError": "60kb"
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            },
            "mocky": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.mocky.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "aot": true,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "revalidation:build",
            "proxyConfig": "proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "revalidation:build:production"
            },
            "staging": {
              "browserTarget": "revalidation:build:staging"
            },
            "mocky": {
              "browserTarget": "revalidation:build:mocky"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "revalidation:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/nhsuk-frontend/packages/assets/",
                "output": "assets/"
              },
              "src/manifest.webmanifest",
              "src/robots.txt"
            ],
            "styles": ["src/styles.scss"],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "cypress/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "hee-shared": {
      "projectType": "library",
      "root": "projects/hee-shared",
      "sourceRoot": "projects/hee-shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/hee-shared/tsconfig.lib.json",
            "project": "projects/hee-shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/hee-shared/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/hee-shared/src/test.ts",
            "tsConfig": "projects/hee-shared/tsconfig.spec.json",
            "karmaConfig": "projects/hee-shared/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/hee-shared/tsconfig.lib.json",
              "projects/hee-shared/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    }
  },
  "defaultProject": "revalidation",
  "cli": {
    "analytics": "c8073553-0f3f-427d-b173-63b07df298c0"
  }
}
