{
  "name": "revalidation",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:misc && ng serve",
    "start:mocky": "npm run start -- --configuration=mocky",
    "build": "node --max_old_space_size=4096 node_modules/@angular/cli/bin/ng build",
    "build:misc": "npm run build:shared && npm run build:browsers",
    "build:shared": "ng build --project=hee-shared",
    "build:browsers": "node supported.browsers.node.ts",
    "test": "ng test --code-coverage=true",
    "test:ci": "npm run test -- --watch=false --browsers=ChromeHeadless",
    "lint": "ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "cypress": "cypress run",
    "cypress-open": "cypress open",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "preinstall": "npx npm-force-resolutions"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.0.5",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "^10.0.5",
    "@angular/compiler": "^10.0.5",
    "@angular/core": "^10.0.5",
    "@angular/forms": "^10.0.5",
    "@angular/material": "^10.1.0",
    "@angular/platform-browser": "^10.0.5",
    "@angular/platform-browser-dynamic": "^10.0.5",
    "@angular/router": "^10.0.5",
    "@angular/service-worker": "^10.0.5",
    "@ngxs/logger-plugin": "^3.7.1",
    "@ngxs/store": "^3.7.2",
    "@sentry/browser": "^6.3.5",
    "angulartics2": "^10.0.0",
    "aws-amplify": "^3.0.22",
    "browserslist": "^4.16.6",
    "caniuse-lite": "^1.0.30001133",
    "file-saver": "^2.0.5",
    "nhsuk-frontend": "^5.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-ng-packagr": "^0.1002.0",
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "~10.0.5",
    "@angular/language-service": "^11.2.12",
    "@aws-amplify/pubsub": "^3.0.25",
    "@ngxs/cli": "^2.0.0",
    "@ngxs/devtools-plugin": "^3.7.1",
    "@react-native-community/netinfo": "^6.0.0",
    "@types/jasmine": "~3.6.10",
    "@types/jasminewd2": "~2.0.9",
    "@types/node": "^15.0.2",
    "browserslist-useragent-regexp": "^3.0.0",
    "buffer": "^6.0.3",
    "codelyzer": "^6.0.2",
    "cypress": "^8.3.1",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.6.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^10.1.2",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.47.0",
    "stylelint": "^13.13.1",
    "stylelint-prettier": "^1.2.0",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.9"
  },
  "resolutions": {
    "minimist": "^1.2.5",
    "yargs-parser": "^18.1.3",
    "buffer": "^5.6.0",
    "terser-webpack-plugin": "^4.1.0"
  }
